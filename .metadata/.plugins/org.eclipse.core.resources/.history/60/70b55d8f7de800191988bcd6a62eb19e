void DC_MOTR_init(void)
{
	/*Set output pins*/
	SET_BIT(DC_MOTOR_IN_PORT, DC_MOTOR_IN1);
	SET_BIT(DC_MOTOR_IN_PORT, DC_MOTOR_IN2);
}
